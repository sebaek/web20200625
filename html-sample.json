		"prefix": "my",
		"body": [
			"<!DOCTYPE html>",
			"<html>",
			"<head>",
			"<script src=\"https://kit.fontawesome.com/a076d05399.js\"></script>",
			"<script src=\"https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js\"></script>",
			"\t<style>",
			"\t</style>",
			"\t<script>",
			"\t\t$(document).ready(function() {",
			"\t\t\t",
			"\t\t});",
			"\t</script>",
			"</head>",
			"<body>",
			"\t$1",
			"</body>",
			"</html>"
		],
		"description": "my snippet"
	}
